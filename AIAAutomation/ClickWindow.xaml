<Window x:Class="AIAAutomation.ClickWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIAAutomation"
        mc:Ignorable="d"
        Title="ClickWindow" Height="400" Width="600">
    <Grid Margin="10,10,2,9">
        <TextBox x:Name="clickmodal_automationidtarget" HorizontalAlignment="Left" Height="22" Margin="109,124,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" x:FieldModifier="public" AcceptsReturn="False"/>
        <Button x:Name="clickmodal_submitbutton" Content="Button" HorizontalAlignment="Left" Margin="252,124,0,0" VerticalAlignment="Top" Width="74" PreviewMouseDown="clickmodal_submit"/>
    </Grid>
</Window>
