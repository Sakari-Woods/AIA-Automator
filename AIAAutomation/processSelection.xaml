<Window x:Class="AIAAutomation.processSelection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIAAutomation"
        mc:Ignorable="d"
        Title="Select Process" Height="450" Width="533"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen">
    <Grid HorizontalAlignment="Left" Width="524">
        <ListBox x:Name="processList" HorizontalAlignment="Left" Height="363" Margin="30,30,0,0" VerticalAlignment="Top" Width="464"/>
        <Button x:Name="selectProcess" Content="Select" HorizontalAlignment="Left" Margin="30,406,0,0" VerticalAlignment="Top" Width="75" Click="selectProcess_Click"/>
        <Button x:Name="cancelProcess" Content="Cancel" HorizontalAlignment="Left" Margin="419,406,0,0" VerticalAlignment="Top" Width="75" Click="cancelProcess_Click"/>

    </Grid>
</Window>
